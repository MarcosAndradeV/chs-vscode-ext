{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "chs",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#directive"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#words"
		},
		{
			"include": "#name"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.chs",
					"match": "\\b(if|else|while|var|set)\\b"
				}
			]
		},
		"directive": {
			"patterns": [
				{
					"name": "comment.line.chs",
					"match": "%.*(def|macro)\\b.*"
				}
			]
		},
		"comments": {
			"match": "#.*$",
			"name": "comment.chs"
		},
		"strings": {
			"name": "string.quoted.double.chs",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.chs",
					"match": "\\\\."
				}
			]
		},
		"words": {
			"name": "entity.name.chs",
			"begin": "[a-zA-Z+=-_!@$%^&*()?\\\\/\\[\\]{}<>]",
			"end": "[\\s:]",
			"patterns": [
				{
					"name": "entity.name.chs",
					"match": "\\\\."
				}
			]
		},
		"numbers": {
			"name": "constant.numeric.chs",
			"begin": "\\d",
			"end": "\\b"
		}
	},
	"scopeName": "source.chs"
}